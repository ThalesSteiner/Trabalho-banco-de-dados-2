-- esse script cria a tabela original

create schema o; --o de original

CREATE TABLE o.Tabelona (
    assists INT,
    GameID VARCHAR,
    GameMode VARCHAR(10) NOT NULL CHECK (GameMode IN ('ARAM','CLASSIC')),
    baronKills INT,
    bountyLevel INT CHECK (bountyLevel BETWEEN 1 AND 19),
    champExperience INT CHECK (champExperience BETWEEN 0 AND 43500),
    champLevel INT CHECK (champLevel BETWEEN 1 AND 18),
    championName VARCHAR(20), --nome mais longo atualmente tem 15 letras
    damageDealtToObjectives INT,
    damageSelfMitigated INT,
    deaths INT,
    dragonKills INT,
    firstBloodAssist BOOLEAN,
    firstBloodKill BOOLEAN,
    gameEndedInSurrender BOOLEAN,
    goldEarned INT,
    goldSpent INT,
    individualPosition VARCHAR(7) CHECK (individualPosition IN ('TOP','MIDDLE','BOTTOM','JUNGLE','UTILITY','INVALID')),
    inhibitorKills INT CHECK (inhibitorKills BETWEEN 0 AND 6),
    inhibitorsLost INT CHECK (inhibitorsLost BETWEEN 0 AND 6),
    item0 INT,
    item1 INT,
    item2 INT,
    item3 INT,
    item4 INT,
    item5 INT,
    item6 INT,
    kills INT,
    lane VARCHAR(6) CHECK (lane IN ('TOP','MIDDLE','BOTTOM','JUNGLE','NONE')),
    largestCriticalStrike INT,
    largestKillingSpree INT,
    longestTimeSpentLiving INT,
    magicDamageDealtToChampions INT,
    magicDamageTaken INT,
    nexusLost INT CHECK (nexusLost IN (0,1)),
    objectivesStolen INT CHECK (objectivesStolen IN (0,1,2)),
    physicalDamageDealtToChampions INT,
    physicalDamageTaken INT,
    spell1Casts INT,
    spell2Casts INT,
    spell3Casts INT,
    spell4Casts INT,
    summoner1Casts INT,
    summoner2Casts INT,
    summoner1Id INT,
    summoner2Id INT,
    summonerId VARCHAR NOT NULL,
    teamEarlySurrendered BOOLEAN,
    teamId INT NOT NULL CHECK (teamId IN (100,200)),
    timeCCingOthers INT,
    timePlayed INT,
    totalDamageDealtToChampions INT,
    totalDamageShieldedOnTeammates INT,
    totalDamageTaken INT,
    totalHeal INT,
    totalHealsOnTeammates INT,
    totalMinionsKilled INT,
    totalTimeCCDealt INT,
    totalTimeSpentDead INT,
    trueDamageDealtToChampions INT,
    trueDamageTaken INT,
    turretTakedowns INT,
    turretsLost INT,
    visionScore INT,
    visionWardsBoughtInGame INT,
    wardsKilled INT,
    wardsPlaced INT,
    win BOOLEAN,
    PRIMARY KEY (GameID, summonerId)
);
